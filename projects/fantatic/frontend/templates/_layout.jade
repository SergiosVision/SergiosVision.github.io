mixin tBottomCardHolder ()
    div.t-bottomInfoCardsHolder
        div.t-bottomInfoCard
            div.t-bottomInfoCardImage
                img(src="/images/bottominfo/wallet.png")
            div.t-bottomInfoCardInfo
                h3.t-bottomInfoCardTitle Оплата
                p.t-bottomInfoCardDescription Несколько вариантов оплаты, в том числе и оплата электронными деньгами

        div.t-bottomInfoCard
            div.t-bottomInfoCardImage
                img(src="/images/bottominfo/rocket.png")
            div.t-bottomInfoCardInfo
                h3.t-bottomInfoCardTitle Доставка
                p.t-bottomInfoCardDescription Несколько вариантов доставки на ваш выбор или самомвывоз

        div.t-bottomInfoCard
            div.t-bottomInfoCardImage
                img(src="/images/bottominfo/guarantee.png")
            div.t-bottomInfoCardInfo
                h3.t-bottomInfoCardTitle Гарантия
                p.t-bottomInfoCardDescription Несколько вариантов оплаты, в том числе и оплата электронными деньгами

        div.t-bottomInfoCard
            div.t-bottomInfoCardImage
                img(src="/images/bottominfo/tools.png")
            div.t-bottomInfoCardInfo
                h3.t-bottomInfoCardTitle Ремонт
                p.t-bottomInfoCardDescription Проверим устройство на неисправность и отремонтируем

doctype html
html(lang="ru-RU")
    head
        meta(charset="utf-8")
        meta(name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0")
        title Fantatic

        link(rel='stylesheet', href="/css/main.min.css")
        link(rel='stylesheet', href="/css/plugins/owl.carousel.min.css")
        link(rel='stylesheet', href="/css/plugins/owl.theme.default.min.css")
        link(rel='stylesheet', href="/css/plugins/jquery.tosrus.all.css")
    body.t-windowLoading.fixed.t-animeOne
        div.t-lightBoxOverlay
        div.wrapper
            div.t-astronaut.animation
            div.t-frame
            div.background.topBackground.animation
            include _header
            block content
            include _footer

        script(src="/js/vendor.min.js")
        script(src="/js/plugins/owl.carousel.min.js")
        script(src="/js/plugins/Hammer.js")
        script(src="/js/plugins/jquery.tosrus.all.min.js")
        script(src="/js/reviewsSlider.js")
        script(src="/js/main.min.js")